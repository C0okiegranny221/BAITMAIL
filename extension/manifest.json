{
  "manifest_version": 3,
  "name": "Gmail Link Extractor",
  "version": "1.0",
  "permissions": ["scripting", "tabs", "activeTab"],
  "background": { "service_worker": "background.js" },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ]
}
